<h2>Available drones</h2>

{{#each drones}}
<div>
    <h3>{{name}}</h3>
    <p>Number of propellers: {{propellers}}</p>
    <p>Max. Speed: {{maxSpeed}}</p>
    <a href="/drones/{{_id}}/edit">Update Drone Info</a>
    <form action="/drones/{{_id}}/delete" method="post">
    <button type="submit">Delete</button>
    </form> 
</div>
{{/each}}